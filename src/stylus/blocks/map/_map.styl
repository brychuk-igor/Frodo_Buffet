.map_block
	height 1100px
	padding-top 135px
	position relative
	&:before
		position absolute
		display block
		content ''
		top -5px
		left -12px
		width 1349px
		height 1075px
		background url(../img/map-bg.png) no-repeat
	&:after
		position absolute
		display block
		content ''
		top -20px
		right 40px
		width 162px
		height 333px
		background url(../img/map-bg-right.png)

.map_block__text_top
	margin-top 70px
	
.map_block__text
	font-size 3.3em
	margin-top 95px
	color #1F1F1F
	text-align center
	line-height 1.5em